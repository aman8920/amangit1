            <label>FirstName *</label>
            <input type="text" name="fn" required autofocus style="margin-left:15px"><br><br>
            
            <label>LastName</label>
            <input type="text" name="ln" style="margin-left:15px"><br><br>
            <label>Email</label>
            <input type="email" name="ln"  placeholder="fname.lname@infinite.com" style="margin-left:15px"><br><br>
            <label>Date of Birth</label>
            <input type="date" ><br><br>
            <input type="submit">
            
